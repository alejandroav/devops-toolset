{
  "init": {
    "replacements": {
      "OWNER": "$GITHUB_OWNER",
      "USERNAME": "$GITHUB_USERNAME",
      "TOKEN": "$GITHUB_TOKEN"
    },
    "file_path": "$PROJECT_API_PATH/nuget.config"
  },
  "build": {
    "path": "$PROJECT_API_PATH",
    "force": true,
    "restore": false,
    "configuration": "Release",
    "framework": "net5.0",
    "runtime": "linux-x64",
    "output": "$PROJECT_API_PATH/build"
  },
  "test": {},
  "pack": {},
  "merge": {},
  "deploy": {
    "path": "$PROJECT_API_PATH/build",
    "force": false,
    "restore": false,
    "configuration": "Release",
    "framework": "net5.0",
    "runtime": "linux-x64",
    "self_contained": false,
    "output": "$PROJECT_API_PATH/publish"}
}